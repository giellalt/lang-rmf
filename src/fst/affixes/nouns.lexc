+N+Fem: FEMOBL ; ! todo

LEXICON RAADZA
!! **LEXICON @LEXNAME@** 
+N+Fem+Sg+Nom:%>a # ;
+N+Fem: FEMOBL ; ! todo



!! ## Morphology

!! ### Masculine morphology

LEXICON MSCOBL
!! **LEXICON @LEXNAME@** 
MSCOBL_SG ;
MSCOBL_PL ;


LEXICON MSCOBL_SG 
!! **LEXICON @LEXNAME@** 
+Sg+Obl:s # ;
+Sg:s OBLCASES_SG ;
+Sg+Gen:s GENCASES ;


LEXICON MSCOBL_PL 
!! **LEXICON @LEXNAME@** 
+Pl+Nom:%>e # ; ! džeene
+Pl+Obl:%>en # ; ! džeenen
+Pl:%>en OBLCASES_PL ;
+Pl+Gen:%>en GENCASES ;

LEXICON MSCOBL_PL_OBL 
!! **LEXICON @LEXNAME@** 
+Pl+Obl:%>en # ; ! džeenen
+Pl:%>en OBLCASES_PL ;
+Pl+Gen:%>en GENCASES ;

LEXICON MSCOBL_PL_o 
!! **LEXICON @LEXNAME@** 
+Pl+Nom:%>i # ; ! džeene
+Pl+Obl:%>on # ; ! džeenen
+Pl:%>on OBLCASES_PL ;
+Pl+Gen:%>on GENCASES ;



!! ### Feminine morphology


LEXICON FEMOBL
!! **LEXICON @LEXNAME@** 
FEMOBL_SG ;
FEMOBL_PL ;


LEXICON FEMOBL_SG 
!! **LEXICON @LEXNAME@** 
+Sg+Obl:ja # ; ! phenja
+Sg+Obl:a # ; ! xxx check this tšimba
+Sg+Obl:e # ; ! xxx check this tšimbe
+Sg:ja OBLCASES_SG ;
+Sg:a OBLCASES_SG ;
+Sg:e OBLCASES_SG ;
+Sg+Gen:ja GENCASES ;
+Sg+Gen:a GENCASES ;
+Sg+Gen:e GENCASES ;

LEXICON FEMOBL_PL 
!! **LEXICON @LEXNAME@** 
+Pl+Nom:%>e # ; ! džeene
+Pl+Obl:%>en # ; ! džeenen
+Pl:%>en OBLCASES_PL ;
+Pl+Gen:%>en GENCASES ;

LEXICON FEMOBL_PL_OBL 
!! **LEXICON @LEXNAME@** 
+Pl+Obl:%>en # ; ! džeenen
+Pl:%>en OBLCASES_PL ;
+Pl+Gen:%>en GENCASES ;

LEXICON FEMOBL_PL_o 
!! **LEXICON @LEXNAME@** 
+Pl+Nom:%>i # ; ! džeene
+Pl+Obl:%>on # ; ! džeenen
+Pl:%>on OBLCASES_PL ;
+Pl+Gen:%>on GENCASES ;


!! ### The oblique cases *-ke, -ta, -ha*

LEXICON OBLCASES_SG
!! **LEXICON @LEXNAME@** 
+Dat:%>ke # ;
+Abl:%>ta # ;
+Ins:%>^CDELha # ;

LEXICON OBLCASES_PL
!! **LEXICON @LEXNAME@** 
+Dat:%>ge # ;
+Abl:%>na # ;
+Ins:%>sa # ;

LEXICON GENCASES
!! **LEXICON @LEXNAME@** 
      :%>ko # ;
+Kiiro:%>kiiro # ; ! fix tags
+Kiiri:%>kiiri # ;
+Kiire:%>kiire # ;


! vim: set ft=xfst-lexc:

