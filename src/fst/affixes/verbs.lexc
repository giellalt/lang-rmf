!! ## Lexical verbs

!! ### r-stems


!! ### Other stems

LEXICON BOLL
!! @LEXNAME@ primary verb, other than r-stem
+Ind+Prs: cns_pressuff ;
+Ind+Prt:%>d pretsuff ;
+PrfPrs:%>d PRFPRS ;

LEXICON TSEER
!! @LEXNAME@ primary verb, r-stem
+Ind+Prs: cns_pressuff ;
+Ind+Prt:%>d pretsuff ;
+PrfPrs:%>d PRFPRS ;

LEXICON TRAAD
+Ind+Prs: cns_pressuff ;
+Ind+Prt:%>id pretsuff ;

LEXICON TSUUV
+Ind+Prs:v cns_pressuff ;
+Ind+Prt:nd%> pretsuff ;
+PrfPrs:nd PRFPRS ;

LEXICON LURR
+Ind+Prs: cns_pressuff ;
+Ind+Prt:%>d pretsuff ;
+PrfPrs:%>d PRFPRS ;

LEXICON DZAMB
+Ind+Prs: cns_pressuff ;
+Ind+Prt:%>id pretsuff ;
+PrfPrs:id PRFPRS ;

LEXICON MANG
+Ind+Prs: cns_pressuff ;
+Ind+Prt: pretsuff_jom ;

LEXICON DIKK
+Ind+Prs: cns_pressuff ;
+Ind+Prt: pretsuff_jom ;

LEXICON KAMM
+Ind+Prs: cns_pressuff ;
+Ind+Prt: pretsuff_jom ;




LEXICON BAHH
