!! ## Person suffixes

LEXICON cns_pressuff
+Sg1:%>aa  #   ;
+Sg2:%>eha  #   ;
+Sg3:%>ela  #   ;
+Pl1:%>ena  #   ;
+Pl2:%>ena  #   ;
+Pl3:%>ena  #   ;

LEXICON vow_pressuff
+Sg1:%>a  #   ;
+Sg2:%>ha  #   ;
+Sg3:%>la  #   ;
+Pl1:%>ha  #   ;
+Pl2:%>na  #   ;
+Pl3:%>na  #   ;


LEXICON pretsuff
+Sg1:%>om  #   ;
+Sg2:%>al  #   ;
+Sg3:%>as  #   ;
+Pl1:%>am  #   ;
+Pl1:%>ommas  #   ;
