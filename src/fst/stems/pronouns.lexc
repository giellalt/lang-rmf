!! # Pronouns
!  --------
!! Three pronouns so far

LEXICON Pronouns
Personal ;
Reflexive ;
Possessive ;

LEXICON Personal 
!! @LEXNAME@ me, jou
me+Pron+Pers: me_pron "minä" ;
tu+Pron+Pers+: tu_pron "sinä" ;
jou+Pron+Pers: jou_pron "hän" ; 

LEXICON Reflexive 
+Pron+Refl:son # ;

LEXICON Possessive 
+Pron+Poss+Sg+Sg1+Msc:mo # ;
+Pron+Poss+Sg+Sg1+Msc:muro # ;
+Pron+Poss+Sg+Sg2+Msc:to # ;
+Pron+Poss+Sg+Sg2+Msc:tiro # ;
+Pron+Poss+Sg+Pl1+Msc:maro # ;
+Pron+Poss+Sg+Pl2+Msc:tumaro # ;

+Pron+Poss+Sg+Sg1+Fem:mi # ;
+Pron+Poss+Sg+Sg1+Fem:muri # ;
+Pron+Poss+Sg+Sg2+Fem:ti # ;
+Pron+Poss+Sg+Sg2+Fem:tiri # ;
+Pron+Poss+Sg+Pl1+Fem:mari # ;
+Pron+Poss+Sg+Pl2+Fem:tumari # ;

+Pron+Poss+Pl+Sg1:me # ;
+Pron+Poss+Pl+Sg1:mure # ;
+Pron+Poss+Pl+Sg2:te # ;
+Pron+Poss+Pl+Sg2:tire # ;
+Pron+Poss+Pl+Pl1:mare # ;
+Pron+Poss+Pl+Pl2:tumare # ;

